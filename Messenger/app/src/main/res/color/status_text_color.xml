<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Focused but not Pressed -->
    <item android:color="#fff" android:state_enabled="true" android:state_pressed="false" />

    <!-- Focused and Pressed -->
    <item android:color="#fff" android:state_enabled="true" android:state_pressed="true" />

    <!-- Default Color -->
    <item android:color="#676565" />

</selector>
